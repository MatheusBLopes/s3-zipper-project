# S3 Zipper Project Environment Configuration
# Copy this file to .env and update with your actual values

AWS_REGION=us-east-1
PROJECT_NAME=s3-zip-jobs

# S3 bucket names (must be globally unique)
SRC_BUCKET=s3-zip-jobs-source-2024
DST_BUCKET=s3-zip-jobs-destination-2024
DST_PREFIX=zips/

# API Gateway URL (filled after terraform apply - get from 'api_base_url' output)
API_BASE=https://your-api-gateway-url.execute-api.us-east-1.amazonaws.com
